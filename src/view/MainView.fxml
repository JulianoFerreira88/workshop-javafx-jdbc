<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<ScrollPane fx:controller="controller.MainController"
            xmlns:fx="http://javafx.com/fxml" >
    <content>
        <VBox prefHeight="326.0" prefWidth="513.0">
            <children>
                <MenuBar>
                    <menus>
                        <Menu mnemonicParsing="false" text="Registration">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Seller" fx:id="menuItemSeller" onAction="#onMenuItemSellerAction"/>
                                <MenuItem mnemonicParsing="false" text="Departments" fx:id="menuItemDepartment" onAction="#onMenuItemDepartmentAction"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About"  fx:id="menuItemAbout" onAction="#onMenuItemAboutAction"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </children>
        </VBox>
    </content>
</ScrollPane>